                        <radio-item value="Book"/>
                        <radio-item value="Article"/>
                        <radio-item value="Music"/>
                        <radio-item value="Movie"/>
                        <radio-item value="Course Packet"/>
                        <radio-item value="Website"/>
                        <radio-item value="Other"/>
