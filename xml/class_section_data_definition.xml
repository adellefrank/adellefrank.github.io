                        <dropdown-item value="ASIN"/>
                        <dropdown-item value="DOI"/>
                        <dropdown-item value="ISAN"/>
                        <dropdown-item value="ISMN"/>
                        <dropdown-item value="ISRC"/>
                        <dropdown-item value="ISSN"/>
                        <dropdown-item value="ISWC"/>
                        <dropdown-item value="UPC"/>
                        <dropdown-item value="Other"/>
