        </text>
        <text wysiwyg="true" identifier="Description" label="Course prerequisites, placement, transfer credit, etc. details"/>
        <group identifier="Area" label="Area" multiple="true">
            <text identifier="Label" label="Label for Course Area"/>
            <asset type="block" identifier="courseBlock" label="Choose an index block of Courses" render-content-depth="2"/>
            <asset type="page" identifier="CatalogCourse" label="Catalog Course" multiple="true" render-content-depth="2"/>
        </group>
    </group>
</system-data-structure>